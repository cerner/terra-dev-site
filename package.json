{
  "name": "terra-dev-site",
  "version": "6.38.0",
  "description": "Dynamically builds a react-hash-routed site based on site configuration, navigation configuration and component configuration.",
  "engines": {
    "node": ">=8.9.2 <14"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-dev-site"
  },
  "keywords": [
    "Cerner",
    "Terra",
    "Terra Dev Site",
    "terra-dev-site",
    "UI"
  ],
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-dev-site/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "/src/**.js",
      "/src/**.jsx"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./jestsetup.js"
    ],
    "testMatch": [
      "**/jest/**/(*.)(spec|test).js?(x)"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "github-markdown-css": "identity-obj-proxy"
    }
  },
  "bin": {
    "tds-generate-app-config": "bin/generateAppConfig.js"
  },
  "eslintConfig": {
    "extends": "@cerner/terra",
    "settings": {
      "polyfills": [
        "Promise"
      ]
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-terra",
    "rules": {
      "scss/at-mixin-pattern": "^(terra-)[a-z]+([a-z0-9-]+[a-z0-9]+)?$"
    }
  },
  "package-json-lint": {
    "extends": "@cerner/package-json-lint-config-terra",
    "rules": {
      "require-no-terra-base-peer-dependency-versions": "error",
      "require-theme-context-versions": "off"
    }
  },
  "scripts": {
    "clean": "rm package-lock.json; rm -rf node_modules",
    "clean:install": "npm run clean && npm install",
    "clean:obsolete-snapshots": "npm test -- -u",
    "compile": "babel src --out-dir lib --copy-files",
    "compile:dev": "webpack",
    "compile:prod": "rm -rf ./build; webpack -p",
    "compile:lowlight": "rm -rf ./build; THEME=clinical-lowlight-theme webpack -p",
    "compile:fusion": "rm -rf ./build; THEME=orion-fusion-theme webpack -p",
    "heroku-postbuild": "npm install --only=dev && npm run compile:prod",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "lint": "npm run lint:js && npm run lint:scss && npm run lint:package-json",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:package-json": "terra package-json-lint",
    "lint:scss": "stylelint src/**/*.scss",
    "precompile": "rm -rf lib",
    "prepare": "npm run compile",
    "prepare-for-release": "terra prepare-for-release",
    "start": "webpack-dev-server",
    "start-prod": "webpack-dev-server -p",
    "start-heroku": "terra express-server --port $PORT --site './build' ",
    "start-static": "npm run compile:prod && terra express-server --site ./build",
    "test": "npm run lint && npm run jest && npm run wdio",
    "tds:generate-app-config": "node bin/generateAppConfig.js",
    "wdio-default": "terra wdio",
    "wdio-locale": "terra wdio --locales de",
    "wdio-lowlight": "terra wdio --themes clinical-lowlight-theme",
    "wdio-fusion": "terra wdio --themes orion-fusion-theme",
    "wdio": "npm run wdio-default && npm run wdio-locale && npm run wdio-lowlight && npm run wdio-fusion"
  },
  "dependencies": {
    "@cerner/duplicate-package-checker-webpack-plugin": "^1.0.0",
    "@cerner/terra-aggregate-translations": "^2.0.1",
    "@mdx-js/loader": "^1.4.5",
    "@mdx-js/mdx": "^1.5.0",
    "@mdx-js/react": "^1.4.5",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^3.0.0",
    "commander": "^3.0.1",
    "css-loader": "^3.2.0",
    "css-selector-tokenizer": "^0.7.3",
    "enhanced-resolve": "^4.1.0",
    "escodegen": "^1.9.1",
    "file-loader": "^4.0.0",
    "fs-extra": "^8.0.0",
    "fuse.js": "^3.4.5",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^3.1.0",
    "keycode-js": "^3.1.0",
    "loader-utils": "^1.2.3",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.kebabcase": "^4.1.1",
    "lodash.startcase": "^4.4.0",
    "marked": "^0.7.0",
    "memory-fs": "0.4.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.35",
    "postcss-assets-webpack-plugin": "^3.0.0",
    "postcss-custom-properties": "^9.1.1",
    "postcss-loader": "^3.0.0",
    "postcss-rtl": "^1.3.3",
    "prism-react-renderer": "^1.0.2",
    "prismjs": "^1.17.1",
    "prop-types": "^15.5.8",
    "raw-loader": "^3.0.0",
    "react-docgen": "^4.1.1",
    "react-intl": "^2.9.0",
    "react-router-dom": "^5.0.0",
    "rehype-slug": "^2.0.3",
    "rehype-urls": "^1.0.0",
    "sass-loader": "^7.0.1",
    "terra-action-footer": "^1.3.0",
    "terra-action-header": "^2.2.0",
    "terra-application": "^1.19.0",
    "terra-button": "^3.12.0",
    "terra-button-group": "^3.5.0",
    "terra-content-container": "^3.0.0",
    "terra-form-select": "^6.0.0",
    "terra-hyperlink": "^2.9.0",
    "terra-icon": "^3.0.0",
    "terra-image": "^3.0.0",
    "terra-infinite-list": "^3.5.0",
    "terra-list": "^4.6.0",
    "terra-menu": "^6.3.0",
    "terra-mixins": "^1.33.0",
    "terra-overlay": "^3.11.0",
    "terra-search-field": "^3.13.0",
    "terra-spacer": "^3.3.0",
    "terra-status-view": "^4.1.0",
    "terser-webpack-plugin": "^1.1.0",
    "to-ast": "^1.0.0",
    "webpack-merge": "^4.1.2"
  },
  "peerDependencies": {
    "@babel/cli": "^7.4.4",
    "core-js": "^3.1.3",
    "@babel/core": "^7.4.4",
    "raf": "^3.4.1",
    "react-dom": "^16.8.5",
    "react": "^16.8.5",
    "regenerator-runtime": "^0.13.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@cerner/browserslist-config-terra": "^3.1.0",
    "@cerner/eslint-config-terra": "^5.0.0",
    "@cerner/package-json-lint": "file:./cerner-package-json-lint-0.0.0.tgz",
    "@cerner/package-json-lint-config-terra": "file:./cerner-package-json-lint-config-terra-0.0.0.tgz",
    "@cerner/terra-cli": "^1.0.0",
    "@cerner/terra-open-source-scripts": "^1.0.1",
    "@cerner/terra-functional-testing": "^1.6.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "check-installed-dependencies": "^1.0.0",
    "core-js": "^3.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^7.0.0",
    "gh-pages": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "loader-runner": "^2.4.0",
    "pacote": "^11.1.11",
    "raf": "^3.4.1",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "regenerator-runtime": "^0.13.2",
    "stylelint": "^10.0.1",
    "stylelint-config-terra": "^3.0.0",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  }
}
