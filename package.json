{
  "name": "@cerner/terra-dev-site",
  "version": "6.30.0",
  "description": "Dynamically builds a react-hash-routed site based on site configuration, navigation configuration and component configuration.",
  "engines": {
    "node": ">=10 <11"
  },
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-dev-site"
  },
  "keywords": [
    "Cerner",
    "Terra",
    "Terra Dev Site",
    "terra-dev-site",
    "UI"
  ],
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-dev-site/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "/src/**.js",
      "/src/**.jsx"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./jestsetup.js"
    ],
    "testMatch": [
      "**/jest/**/(*.)(spec|test).js?(x)"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "github-markdown-css": "identity-obj-proxy"
    }
  },
  "eslintConfig": {
    "extends": "@cerner/terra",
    "settings": {
      "polyfills": [
        "Promise"
      ]
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-terra",
    "rules": {
      "scss/at-mixin-pattern": "^(terra-)[a-z]+([a-z0-9-]+[a-z0-9]+)?$"
    }
  },
  "files": [
    "lib",
    "src",
    "translations"
  ],
  "scripts": {
    "clean": "rm package-lock.json; rm -rf node_modules",
    "clean:install": "npm run clean && npm install",
    "clean:obsolete-snapshots": "npm test -- -u",
    "compile": "babel src --out-dir lib --copy-files  --ignore src/webpack",
    "compile:dev": "webpack",
    "compile:prod": "rm -rf ./build; webpack -p",
    "compile:lowlight": "rm -rf ./build; THEME=clinical-lowlight-theme webpack -p",
    "compile:fusion": "rm -rf ./build; THEME=orion-fusion-theme webpack -p",
    "heroku-postbuild": "npm install --only=dev && npm run compile:prod",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:scss": "stylelint src/**/*.scss",
    "precompile": "rm -rf lib",
    "prepare": "npm run compile",
    "prepare-for-release": "terra prepare-for-release",
    "start": "webpack-dev-server",
    "start-prod": "webpack-dev-server -p",
    "start-heroku": "terra express-server --port $PORT --site './build' ",
    "start-static": "npm run compile:prod && terra express-server --site ./build",
    "test": "npm run lint && npm run jest && npm run wdio",
    "wdio-default": "wdio",
    "wdio-locale": "LOCALE=de wdio",
    "wdio-lowlight": "THEME=clinical-lowlight-theme wdio",
    "wdio-fusion": "THEME=orion-fusion-theme wdio",
    "wdio": "npm run wdio-default && npm run wdio-locale && npm run wdio-lowlight && npm run wdio-fusion"
  },
  "dependencies": {
    "@cerner/terra-docs": "^1.0.0",
    "@mdx-js/loader": "^1.4.5",
    "@mdx-js/mdx": "^1.5.0",
    "@mdx-js/react": "^1.4.5",
    "classnames": "^2.2.5",
    "enhanced-resolve": "^4.1.0",
    "fuse.js": "^6.4.3",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^4.5.0",
    "keycode-js": "^3.1.0",
    "loader-utils": "^1.2.3",
    "lodash.kebabcase": "^4.1.1",
    "lodash.startcase": "^4.4.0",
    "lodash.template": "^4.5.0",
    "prism-react-renderer": "^1.0.2",
    "prop-types": "^15.5.8",
    "react-docgen": "^5.3.0",
    "react-router-dom": "^5.0.0",
    "rehype-slug": "^4.0.0",
    "rehype-urls": "^1.0.0",
    "terra-action-footer": "^2.3.0",
    "terra-button": "^3.12.0",
    "terra-form-select": "^6.0.0",
    "terra-html-table": "^1.8.0",
    "terra-hyperlink": "^2.9.0",
    "terra-icon": "^3.0.0",
    "terra-infinite-list": "^3.5.0",
    "terra-list": "^4.6.0",
    "terra-menu": "^6.3.0",
    "terra-mixins": "^1.33.0",
    "terra-search-field": "^3.13.0",
    "terra-status-view": "^4.1.0"
  },
  "peerDependencies": {
    "@cerner/terra-application": "cerner/terra-application#30f1e0a57f93ba119f08d4b95edd8ae4d597a866",
    "react-dom": "^16.8.5",
    "react": "^16.8.5",
    "webpack": "^4.28.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@cerner/browserslist-config-terra": "^3.1.0",
    "@cerner/eslint-config-terra": "^4.0.0",
    "@cerner/terra-application": "cerner/terra-application#30f1e0a57f93ba119f08d4b95edd8ae4d597a866",
    "@cerner/terra-cli": "^1.1.0",
    "@cerner/terra-open-source-scripts": "^1.1.0",
    "@cerner/webpack-config-terra": "^1.0.0-alpha.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "core-js": "^3.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^6.1.0",
    "gh-pages": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "loader-runner": "^4.1.0",
    "pacote": "^11.1.11",
    "react": "^16.14.0",
    "react-dom": "^16.8.5",
    "react-intl": "^2.9.0",
    "regenerator-runtime": "^0.13.2",
    "stylelint": "^10.0.1",
    "stylelint-config-terra": "^3.0.0",
    "terra-toolkit": "^6.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^5.3.0"
  }
}
