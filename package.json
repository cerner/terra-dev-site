{
  "name": "terra-dev-site",
  "version": "0.4.2",
  "description": "Dynamically builds a react-hash-routed site based on site configuration, navigation configuration and component configuration.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-dev-site"
  },
  "keywords": [
    "Cerner",
    "Terra",
    "Terra Dev Site",
    "terra-dev-site",
    "UI"
  ],
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-dev-site/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "/src/**.js",
      "/src/**.jsx"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "github-markdown-css": "identity-obj-proxy"
    }
  },
  "bin": {
    "generate-config": "./bin/generate-config.js"
  },
  "scripts": {
    "clean": "node scripts/clean-modules/index.js",
    "clean:install": "npm run clean && npm install",
    "clean:obsolete-snapshots": "npm test -- -u",
    "compile:dev": "webpack --config src/webpack/webpack.config -d",
    "compile:prod": "webpack --config src/webpack/webpack.config -p",
    "danger": "danger",
    "heroku-postbuild": "npm install --only=dev && npm run compile:prod",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:scss": "stylelint src/**/*.scss",
    "prepublishOnly": "npm test",
    "pretest": "npm run lint",
    "release:major": "node scripts/release/release.js major",
    "release:minor": "node scripts/release/release.js minor",
    "release:patch": "node scripts/release/release.js patch",
    "start": "tt:serve --config src/webpack/webpack.config",
    "start-static": "tt:serve-static --config src/webpack/webpack.config",
    "start-heroku": "tt:serve-static --site build",
    "test": "npm run jest"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "classnames": "^2.2.5",
    "commander": "^2.9.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^3.1.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.startcase": "^4.4.0",
    "postcss": "^6.0.21",
    "prop-types": "^15.5.8",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "terra-application-layout": "^1.0.0",
    "terra-base": "^3.3.0",
    "terra-button": "^2.0.1",
    "terra-i18n": "^2.3.0",
    "terra-icon": "^2.0.1",
    "terra-image": "^2.0.1",
    "terra-markdown": "^2.0.0",
    "terra-slide-panel": "^2.0.1",
    "terra-theme-provider": "^2.0.0",
    "terra-toolkit": "git://github.com/cerner/terra-toolkit.git#webpack4",
    "webpack-merge": "^4.1.2",
    "xfc": "^1.2.1"
  },
  "peerDependencies": {
    "babel-polyfill": "^6.23.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-jest": "^21.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "check-installed-dependencies": "^1.0.0",
    "danger": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "gh-pages": "^0.12.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.1",
    "shelljs": "^0.7.6",
    "stylelint": "^8.0.0",
    "stylelint-config-sass-guidelines": "^4.0.1",
    "stylelint-order": "^0.7.0",
    "stylelint-scss": "^2.0.0",
    "stylelint-suitcss": "^1.0.0"
  }
}
