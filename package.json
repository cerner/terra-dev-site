{
  "name": "terra-dev-site",
  "version": "5.0.0",
  "description": "Dynamically builds a react-hash-routed site based on site configuration, navigation configuration and component configuration.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-dev-site"
  },
  "keywords": [
    "Cerner",
    "Terra",
    "Terra Dev Site",
    "terra-dev-site",
    "UI"
  ],
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-dev-site/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "/src/**.js",
      "/src/**.jsx"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "github-markdown-css": "identity-obj-proxy"
    }
  },
  "bin": {
    "tds-generate-app-config": "bin/generateAppConfig.js"
  },
  "browserslist": [
    "extends browserslist-config-terra"
  ],
  "eslintConfig": {
    "extends": "terra",
    "settings": {
      "polyfills": [
        "promises"
      ]
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-terra",
    "rules": {
      "scss/at-mixin-pattern": "^(terra-)[a-z]+([a-z0-9-]+[a-z0-9]+)?$"
    }
  },
  "scripts": {
    "clean": "rm package-lock.json; rm -rf node_modules",
    "clean:install": "npm run clean && npm install",
    "clean:obsolete-snapshots": "npm test -- -u",
    "compile": "babel src --out-dir lib --copy-files",
    "compile:dev": "webpack --config config/webpack/webpack.config",
    "compile:prod": "webpack --config config/webpack/webpack.config -p",
    "heroku-postbuild": "npm install --only=dev && npm run compile:prod",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:scss": "stylelint src/**/*.scss",
    "postpublish": "git push --tag",
    "precompile": "rm -rf lib",
    "prepare": "npm run compile",
    "prepublishOnly": "npm whoami && check-installed-dependencies && npm test",
    "release:major": "npm version major -m 'Released version %s' && npm publish",
    "release:minor": "npm version minor -m 'Released version %s' && npm publish",
    "release:patch": "npm version patch -m 'Released version %s' && npm publish",
    "start": "tt-serve --config config/webpack/webpack.config",
    "start-heroku": "tt-serve-static --port $PORT --site './build' ",
    "start-static": "npm run compile:prod && tt-serve-static --site ./build",
    "test": "npm run lint && npm run jest && npm run wdio",
    "tds:generate-app-config": "node bin/generateAppConfig.js",
    "wdio": "wdio node_modules/terra-toolkit/config/wdio/wdio.conf.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "commander": "^2.9.0",
    "escodegen": "^1.9.1",
    "fs-extra": "^8.0.0",
    "fuse.js": "^3.4.5",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^3.1.0",
    "keycode-js": "^1.0.4",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.kebabcase": "^4.1.1",
    "lodash.startcase": "^4.4.0",
    "prop-types": "^15.5.8",
    "react-intl": "^2.9.0",
    "react-router-dom": "^5.0.0",
    "terra-action-footer": "^2.3.0",
    "terra-action-header": "^2.2.0",
    "terra-aggregate-translations": "^1.0.0",
    "terra-application": "^1.0.0",
    "terra-application-navigation": "^1.0.0",
    "terra-button": "^3.12.0",
    "terra-button-group": "^3.5.0",
    "terra-content-container": "^3.0.0",
    "terra-form-select": "^5.15.0",
    "terra-hyperlink": "^2.9.0",
    "terra-icon": "^3.0.0",
    "terra-image": "^3.0.0",
    "terra-infinite-list": "^3.5.0",
    "terra-list": "^4.6.0",
    "terra-markdown": "^2.26.0",
    "terra-menu": "^6.3.0",
    "terra-overlay": "^3.11.0",
    "terra-search-field": "^3.13.0",
    "terra-spacer": "^3.3.0",
    "terra-status-view": "^4.1.0",
    "to-ast": "^1.0.0",
    "webpack-merge": "^4.1.2"
  },
  "peerDependencies": {
    "react-dom": "^16.8.5",
    "react": "^16.8.5",
    "terra-toolkit": "^5.2.0",
    "webpack": "^4.28.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "browserslist-config-terra": "^1.2.0",
    "check-installed-dependencies": "^1.0.0",
    "core-js": "^3.1.3",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.2.2",
    "enzyme": "^3.3.0",
    "eslint-config-terra": "^2.1.0",
    "eslint": "^5.0.0",
    "gh-pages": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "raf": "^3.4.1",
    "react-dom": "^16.8.5",
    "react": "^16.8.5",
    "regenerator-runtime": "^0.13.2",
    "stylelint-config-terra": "^3.0.0",
    "stylelint": "^10.0.1",
    "terra-toolkit": "^5.2.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack": "^4.28.1"
  }
}
